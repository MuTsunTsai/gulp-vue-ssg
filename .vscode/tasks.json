{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "gulp",
			"task": "default",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"echo": true,
				"reveal": "never",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			},
			"problemMatcher": {
				"fileLocation": "relative",
				"pattern": {
					"regexp": "^([^\\s].*)\\((\\d+,\\d+)\\):\\s+(error|warning|info)\\s+(TS\\d+)\\s*:\\s*(.*)$",
					"file": 1,
					"location": 2,
					"severity": 3,
					"code": 4,
					"message": 5
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": "^\\[.{8}\\]\\sStarting\\s\\'default",
					"endsPattern": "^\\[.{8}\\]\\sFinished\\s\\'default"
				}
			},
			"label": "gulp: build"
		}
	]
}
